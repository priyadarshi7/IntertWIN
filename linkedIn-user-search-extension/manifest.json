{
    "manifest_version": 3,
    "name": "LinkedIn User Redirector",
    "version": "1.0",
    "description": "Redirect to user profile based on LinkedIn URL.",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "http://localhost:8000/",
        "http://localhost:5173/"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}
