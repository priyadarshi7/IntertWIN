{
    "manifest_version": 3,
    "name": "IntertWin Mini",
    "version": "1.0",
    "description": "Anayze your candidates as never before.",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "http://localhost:8000/",
        "http://localhost:5173/"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}
